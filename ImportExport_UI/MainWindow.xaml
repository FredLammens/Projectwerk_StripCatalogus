<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImportExport_UI"
        xmlns:ViewModel="clr-namespace:ViewModel;assembly=ViewModel" x:Class="ImportExport_UI.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="100" Width="250">
    <Grid>
        <Grid.DataContext>
            <ViewModel:ImportExportViewModel/>
        </Grid.DataContext>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button x:Name="btnImport" Content="Import" MinWidth="100" Command="{Binding ImportCommand}" />
            <Button x:Name="btnExport" Content="Export" MinWidth="100" Margin="10,0,0,0" Command="{Binding ExportCommand}"/>
        </StackPanel>
    </Grid>
</Window>
