<Window x:Class="Projectwerk_StripCatalogus.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="StripCatalogus" Height="500" Width="1000">
    <Grid>
        <StackPanel>
            <Menu DockPanel.Dock="Top">
            <MenuItem Header="_Catalogue" Click="BtnCatalogue_Click"/>
            <MenuItem Header="_Add Comic" Click="BtnAddComic_Click"/>
            <MenuItem Header="_Add Others" Click="btnAddOthers_Click"/>
            <MenuItem Header="_Update Others" Click="btnUpdateOthers_Click"/>
        </Menu>
        </StackPanel>
        <Frame x:Name="Main" Margin="0,35,0,0" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
